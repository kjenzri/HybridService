      <ProjectItem ReplaceParameters="true" >ProjectInstaller.Designer.cs</ProjectItem>
      <ProjectItem ReplaceParameters="true" >ProjectInstaller.resx</ProjectItem>
      <ProjectItem ReplaceParameters="true" >packages.config</ProjectItem>
    </Project>
  </TemplateContent>
</VSTemplate>